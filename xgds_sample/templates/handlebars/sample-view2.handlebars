<div> <strong>Sample</strong> <a id="edit_sample_button" href="/xgds_sample/edit/{{pk}}" class="mybutton small" target="_blank">Edit</a> </div>
	<table>
		<tr>
			<th><label for="id_name">Name:</label></th>
			<td>{{name}}</td>
		</tr>
		<tr>
			<th><label for="id_type">Type:</label></th>
			<td>{{sample_type}}</td>
		</tr>
		<tr>
			<th><label for="id_region">Region:</label></th>
			<td>{{region}}</td>
		</tr>
		<tr>
			<th><label for="id_resource">Resource:</label></th>
			<td>{{resource}}</td>
		</tr>
		<tr>
			<th><label for="id_collector">Collector:</label></th>
			<td>{{collector}}</td>
		</tr>
		<tr>
			<th><label for="id_number">Number:</label></th>
			<td>{{number}}</td>
		</tr>
		<tr>
			<th><label for="id_replicate">Replicate:</label></th>
			<td>{{replicate}}</td>
		</tr>	
		<tr>
			<th><label for="id_year">Year:</label></th>
			<td> {{year}}</td>
		</tr>
		<tr>
			<th><label for="id_flight">EVA:</label></th>
			<td>{{flight}}</td>
		</tr>
		<tr>
			<th><label for="id_latitude">Latitude:</label></th>
			<td>{{lat}}</td>
		</tr>
		<tr>
			<th><label for="id_longitude">Longitude:</label></th>
			<td>{{lon}}</td>
		</tr>
		<tr>
			<th><label for="id_altitude">Altitude:</label></th>
			<td>{{alt}}</td>
		</tr>
		<tr>
			<th><label for="id_collection_time">Collection time:</label></th>
			<td>{{prettyTime collection_time collection_timezone}}</td>
		</tr>
		<tr>
			<th><label for="id_description">Description:</label></th>
			<td>{{description}}</td>
		</tr>
	</table>
